
@{
    ViewBag.Title = "New Assignment";
}

<h2>New Assignment</h2>

<form id="newRental">
    <div class="form-group">
        <label>Nombre del solicitante</label>
        <div class="tt-container">
            <input id="nombre" name="nombre" data-rule-validCustomer="true" required type="text" value="" class="form-control" />
        </div>
    </div>
    <div class="form-group">
        <label>Proyecto</label>
        <div class="tt-container">
            <input id="proyecto" name="proyecto" data-rule-atLeastOneMovie="true" type="text" value="" class="form-control" />
        </div>
    </div>
    <div class="form-group">
        <label>Responsable del Equipo</label>
        <div class="tt-container">
            <input id="responsable" name="responsable" data-rule-atLeastOneMovie="true" type="text" value="" class="form-control" />
        </div>
    </div>
    <label>Sede</label>
    <select id="Sede">
        <option>Guadalajara</option>
        <option>Aguascalientes</option>
    </select>

    <pre><button class="btn btn-primary">Submit</button></pre>
</form>

  
